Animacionessssssssssss

@keyframes mover-y-cambiar-color {
    0% {
      background-color: blue; /* Color inicial */
      transform: translateX(0); /* Posición inicial */
    }
    50% {
      background-color: red; /* Color intermedio */
      transform: translateX(50px); /* Posición intermedia */
    }
    100% {
      background-color: blue; /* Color final */
      transform: translateX(100px); /* Posición final */
    }
  }
  
  .animated-square {
    width: 50px;
    height: 50px;
    background-color: blue;
    animation-name: mover-y-cambiar-color;
    animation-duration: 3s; /* Duración de la animación */
    animation-iteration-count: infinite; /* Repetir infinitamente */
  }


